<style>
    table,th,tr,td{
        border: solid red 1px;
    }
</style>

<button> Crear </button>
<table style="
width: 100%">
        <tr>
            <th>Nombre</th>
            <th>Grados</th>
            <th>Caduca</th>
            <th>Precio</th>
            <th>Tipo</th>
            <th>Acciones</th>
        </tr>
        <% arreglosTragos.forEach((trago)=>{ %>
            <tr>
                <td> <%=trago.nombre%> </td>
                <td> <%=trago.gradoAlcohol%> </td>
                <td> <%=trago.fechaCaducidad%> </td>
                <td> <%=trago.precio%> </td>
                <td> <%=trago.tipo%> </td>
                <td>
                    <button>Editar</button>
                    <button>Eliminar</button>
                </td>
            </tr>
        <% })%>
    </table>